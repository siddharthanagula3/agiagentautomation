{
  "name": "agi-agent-automation",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "prepare": "husky",
    "vibe:migrate": "tsx scripts/apply-migration-via-netlify.ts",
    "vibe:test": "tsx scripts/test-vibe-integration.ts",
    "vibe:deploy": "bash scripts/vibe-deployment-workflow.sh",
    "size": "size-limit",
    "size:check": "size-limit --json",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --collect.startServerCommand='npm run preview' --collect.url='http://localhost:4173/'",
    "security:audit": "npm audit --audit-level=high",
    "security:audit:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=critical && echo 'No critical vulnerabilities found'"
  },
