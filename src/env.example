# Environment Variables Example
# Copy this file to .env in the root directory and fill in your actual values
# NEVER commit the .env file to version control

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Keys (Get these from respective provider dashboards)
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# JWT Secret (for development - generate a secure random string)
VITE_JWT_SECRET=your_jwt_secret_here

# API Configuration
VITE_API_URL=http://localhost:8888
VITE_WS_URL=ws://localhost:8888

# Demo Mode (optional)
VITE_DEMO_MODE=false

# Development Settings
NODE_ENV=development

# Netlify Functions Environment Variables (for production)
# These are set in Netlify dashboard, not in .env file
# STRIPE_SECRET_KEY=your_stripe_secret_key_here
# STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here
# SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
